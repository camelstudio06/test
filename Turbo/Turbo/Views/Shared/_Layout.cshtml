                    </div>
                </a>
            </div>
            <div class="col-12 d-flex justify-content-end">
                <p class="register d-flex align-items-center">
                    @if (!User.Identity.IsAuthenticated)
                    {
                        <a asp-controller="Account" asp-action="Login"> <i class="fas fa-user userIcon"></i>Giriş</a>
                        <a asp-controller="Account" asp-action="Register">Qeydiyyat</a>
                    }
                    else
                    {
                        <a asp-controller="Announcement" asp-action="MyLists" asp-route-name="@User.Identity.Name" class="mr-3 px-3" style="background-color: blue; color:yellow;">Elanlarım</a>
                        <span class="mr-3 px-3" style="background-color: white;">@User.Identity.Name</span>
                        <a asp-controller="Account" asp-action="Logout">Hesabdan çıx</a>
                    }
                </p>
            </div>
